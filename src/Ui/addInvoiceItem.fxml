<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="230.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AddInvoiceItemController">
   <children>
      <Label fx:id="invoiceItemTitle" text="Add invoice item to database">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="invoiceItemInvoiceId" alignment="CENTER" maxWidth="150.0" promptText="Invoice Id" />
      <TextField fx:id="invoiceItemProductId" alignment="CENTER" maxWidth="150.0" promptText="Product Id" />
      <Button fx:id="invoiceItemAdd" mnemonicParsing="false" onAction="#addInvoiceItem" text="Submit" />
   </children>
</VBox>
